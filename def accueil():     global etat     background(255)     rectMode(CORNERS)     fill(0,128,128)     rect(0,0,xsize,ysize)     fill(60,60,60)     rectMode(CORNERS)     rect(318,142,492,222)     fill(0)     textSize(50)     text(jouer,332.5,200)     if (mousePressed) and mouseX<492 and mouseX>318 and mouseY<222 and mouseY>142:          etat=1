def accueil():
    global etat
    background(255)
    rectMode(CORNERS)
    fill(0,128,128)
    rect(0,0,xsize,ysize)
    fill(60,60,60)
    rectMode(CORNERS)
    rect(318,142,492,222)
    fill(0)
    textSize(50)
    text(jouer,332.5,200)
    if (mousePressed) and mouseX<492 and mouseX>318 and mouseY<222 and mouseY>142: 
        etat=1
